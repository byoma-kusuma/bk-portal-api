{
  "name": "nestjs-prisma-client-starter",
  "version": "0.0.1",
  "description": "NestJS Prisma Client Starter Project",
  "author": "Marc Stammerjohann",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript",
    "passport",
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/byoma-kusuma/bk-portal-api"
  },
  "bugs": {
    "url": "https://github.com/byoma-kusuma/bk-portal-api/issues"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build && npm run test",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:azure": "npm run build && func host start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev",
    "migrate:dev:create": "prisma migrate dev --create-only",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "npx prisma migrate deploy",
    "migrate:status": "npx prisma migrate status",
    "migrate:resolve": "npx prisma migrate resolve",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "prisma:db:reset:force": "npx prisma db push --force-reset",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "prisma db seed",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "sonar": "ts-node sonar-scanner.ts"
  },
  "dependencies": {
    "@azure/functions": "^1.0.3",
    "@devoxa/prisma-relay-cursor-connection": "2.0.3",
    "@golevelup/ts-jest": "^0.3.4",
    "@nestjs/apollo": "^10.0.2",
    "@nestjs/azure-func-http": "^0.8.0",
    "@nestjs/common": "8.2.5",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "8.2.5",
    "@nestjs/graphql": "10.0.2",
    "@nestjs/jwt": "8.0.0",
    "@nestjs/passport": "8.1.0",
    "@nestjs/platform-express": "8.2.5",
    "@nestjs/swagger": "5.1.5",
    "@nestjs/throttler": "^3.0.0",
    "@prisma/client": "3.8.1",
    "@types/nodemailer": "^6.4.5",
    "apollo-server-express": "^3.6.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "crypto-random-string": "^5.0.0",
    "dayjs": "^1.11.5",
    "graphql": "^15.6.0",
    "graphql-scalars": "^1.14.1",
    "graphql-subscriptions": "1.2.1",
    "nestjs-prisma": "^0.16.0",
    "nodemailer": "^6.7.8",
    "passport": "0.5.2",
    "passport-jwt": "4.0.0",
    "prisma-nestjs-graphql": "^16.0.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.5.2",
    "swagger-ui-express": "4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "8.2.0",
    "@nestjs/testing": "8.2.5",
    "@paljs/plugins": "^4.1.0",
    "@types/bcrypt": "5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^4.0.1",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "14.17.3",
    "@types/passport-jwt": "^3.0.7",
    "@types/supertest": "2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "chance": "1.1.8",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "prisma": "3.8.1",
    "prisma-dbml-generator": "0.8.3",
    "sonarqube-scanner": "^2.8.2",
    "supertest": "6.2.1",
    "ts-jest": "27.1.3",
    "ts-loader": "9.2.6",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.12.0",
    "typescript": "4.5.4"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
